<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/shape"
    tools:context=".MainActivity" ><![CDATA[


        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

    ]]>

    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="33dp"
                android:layout_height="38dp"
                android:layout_marginTop="36dp"
                android:layout_marginEnd="360dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/hum" />

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="174dp"
                android:layout_height="159dp"
                android:layout_marginTop="80dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.493"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/yoga_logo" />

            <TextView
                android:id="@+id/welcome"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Welcome"
                android:textColor="@color/black"
                android:textSize="30dp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteY="34dp" />

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="414dp"
                android:layout_height="470dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="1.0"
                app:srcCompat="@drawable/containert" />


            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/button6"
                android:layout_width="167dp"
                android:layout_height="164dp"
                android:layout_weight="1"
                android:background="@drawable/nuetri"
                tools:layout_editor_absoluteX="32dp"
                tools:layout_editor_absoluteY="517dp" />



            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/button5"
                android:layout_width="168dp"
                android:layout_height="176dp"
                android:layout_weight="1"
                tools:layout_editor_absoluteX="32dp"
                tools:layout_editor_absoluteY="287dp"
                android:background="@drawable/woman"/>

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/button4"
                android:layout_width="168dp"
                android:layout_height="176dp"
                android:layout_weight="1"
                android:background="@drawable/wei"
                android:text="Button"
                tools:layout_editor_absoluteX="218dp"
                tools:layout_editor_absoluteY="505dp" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/button3"
                android:layout_width="168dp"
                android:layout_height="176dp"
                android:layout_weight="1"
                tools:layout_editor_absoluteX="218dp"
                tools:layout_editor_absoluteY="287dp"
                android:background="@drawable/water"/>

            <TextView
                android:id="@+id/textView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Exercise"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="92dp"
                tools:layout_editor_absoluteY="475dp" />

            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="16dp"
                android:text="Nutrition"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                tools:layout_editor_absoluteX="90dp" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Water Tracker"
            android:textStyle="bold"
            tools:layout_editor_absoluteX="259dp"
            tools:layout_editor_absoluteY="475dp" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="16dp"
            android:text="Weight Tracker"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            tools:layout_editor_absoluteX="253dp" />

    </androidx.constraintlayout.widget.ConstraintLayout>









</androidx.constraintlayout.widget.ConstraintLayout>
